{
  "version": 2,
  "name": "admin-mini-ecommerce",
  "builds": [
    {
      "src": "apps/admin/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/apps/admin/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://mini-ecommerce-software.onrender.com/api",
    "NODE_ENV": "production"
  }
}
